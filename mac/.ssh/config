Host name1
  HostName name1.example.org
Host name2
  HostName name2.example.org
Host name3
  HostName name3.example.org
Host *
  AddKeysToAgent yes
  CanonicalizeHostname yes
  ForwardAgent yes
  IdentityFile ~/.ssh/id_rsa
  # ServerAliveInterval set to 30 minutes
  ServerAliveInterval 1800
  UseKeychain yes
